<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<Preference
			android:title="@string/version"
			android:summary="@string/error"
			android:key="debug_version"/>
	<Preference
			android:title="@string/build_date"
			android:summary="@string/error"
			android:key="debug_build_date"/>
	<Preference
			android:title="@string/clear_cache"
			android:summary="@string/clear_cache_info"
			android:key="debug_clear_cache" />
	<Preference
			android:title="@string/clear_settings"
	        android:summary="@string/clear_settings_info"
	        android:key="debug_clear_settings"/>
	<CheckBoxPreference
		android:title="Use fake data"
	    android:summary="Supplies false data to the grades checking system. This affects the cache."
	    android:key="debug_false_aeries_data"/>
</PreferenceScreen>